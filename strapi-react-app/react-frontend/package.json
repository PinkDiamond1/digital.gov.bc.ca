{
  "name": "testreact",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "plugins": [
      "react"
    ],
    "extends": [
      "react-app",
      "eslint:all"
    ],
    "rules": {
      "semi": ["error", "always"],
      "no-cond-assign": ["error", "always"],
      "react/no-set-state": "off",
      "sort-imports": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "husky": "^4.2.5",
    "lint-staged": "^10.2.6",
    "typescript": "^3.9.3"
  },
  "lint-staged": {
    "src/components/*.{js,jsx}": [
      "eslint . --fix"
    ],
    "src/**/*.scss": [
      "stylelint --syntaxscss --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
